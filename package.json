  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-customizable": "^5.0.0",
    "husky": "^0.13.3",
    "standard": "^9.0.2",
    "validate-commit-msg": "^2.11.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
